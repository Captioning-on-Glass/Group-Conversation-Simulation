#ifndef EIGEN_ITERATIVE_SOLVERS_MODULE_H
#define EIGEN_ITERATIVE_SOLVERS_MODULE_H

#include "eigen-3.4.0/Eigen/Sparse"
#include "eigen-3.4.0/Eigen/Jacobi"
#include "eigen-3.4.0/Eigen/Householder"


/**
  * \defgroup IterativeLinearSolvers_Module Iterative solvers module
  * This module aims to provide various iterative linear and non linear solver algorithms.
  * It currently provides:
  *  - a constrained conjugate gradient
  *  - a Householder GMRES implementation
  *  - an IDR(s) implementation
  *  - a DGMRES implementation
  *  - a MINRES implementation
  *
  * \code
  * #include <unsupported/Eigen/IterativeSolvers>
  * \endcode
  */


#include "eigen-3.4.0/Eigen/src/Core/util/DisableStupidWarnings.h"

#ifndef EIGEN_MPL2_ONLY
#include "eigen-3.4.0/unsupported/Eigen/src/IterativeSolvers/IterationController.h"
#include "eigen-3.4.0/unsupported/Eigen/src/IterativeSolvers/ConstrainedConjGrad.h"
#endif

#include "eigen-3.4.0/unsupported/Eigen/src/IterativeSolvers/IncompleteLU.h"
#include "eigen-3.4.0/unsupported/Eigen/src/IterativeSolvers/GMRES.h"
#include "eigen-3.4.0/unsupported/Eigen/src/IterativeSolvers/DGMRES.h"
//#include "src/IterativeSolvers/SSORPreconditioner.h"
#include "eigen-3.4.0/unsupported/Eigen/src/IterativeSolvers/MINRES.h"
#include "eigen-3.4.0/unsupported/Eigen/src/IterativeSolvers/IDRS.h"

#include "eigen-3.4.0/Eigen/src/Core/util/ReenableStupidWarnings.h"


#endif // EIGEN_ITERATIVE_SOLVERS_MODULE_H
